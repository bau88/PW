<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.fpuna.pweb.server.GreetingServiceImpl</servlet-class>
  </servlet>
    
  <servlet>
    <servlet-name>cajaServlet</servlet-name>
    <servlet-class>com.fpuna.pweb.server.CajaServiceImpl</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>clienteServlet</servlet-name>
    <servlet-class>com.fpuna.pweb.server.ClienteServiceImpl</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>compraServlet</servlet-name>
    <servlet-class>com.fpuna.pweb.server.CompraServiceImpl</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>facturaServlet</servlet-name>
    <servlet-class>com.fpuna.pweb.server.FacturaServiceImpl</servlet-class>
  </servlet>
  
  
  <servlet>
    <servlet-name>pagoServlet</servlet-name>
    <servlet-class>com.fpuna.pweb.server.PagoServiceImpl</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>productoServlet</servlet-name>
    <servlet-class>com.fpuna.pweb.server.ProductoServiceImpl</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>proveedorServlet</servlet-name>
    <servlet-class>com.fpuna.pweb.server.ProveedorServiceImpl</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>rolServlet</servlet-name>
    <servlet-class>com.fpuna.pweb.server.RolServiceImpl</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>usuarioServlet</servlet-name>
    <servlet-class>com.fpuna.pweb.server.UsuarioServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/tpfinal_gwt/greet</url-pattern>
  </servlet-mapping>

  
  <servlet-mapping>
    <servlet-name>cajaServlet</servlet-name>
    <url-pattern>/tpfinal_gwt/servicioCaja</url-pattern>
  </servlet-mapping>
  
   
  <servlet-mapping>
    <servlet-name>clienteServlet</servlet-name>
    <url-pattern>/tpfinal_gwt/servicioCliente</url-pattern>
  </servlet-mapping>  
   
  <servlet-mapping>
    <servlet-name>compraServlet</servlet-name>
    <url-pattern>/tpfinal_gwt/servicioCompra</url-pattern>
  </servlet-mapping>  
   
  <servlet-mapping>
    <servlet-name>facturaServlet</servlet-name>
    <url-pattern>/tpfinal_gwt/servicioFactura</url-pattern>
  </servlet-mapping>  
   
  <servlet-mapping>
    <servlet-name>pagoServlet</servlet-name>
    <url-pattern>/tpfinal_gwt/servicioPago</url-pattern>
  </servlet-mapping>  
   
  <servlet-mapping>
    <servlet-name>productoServlet</servlet-name>
    <url-pattern>/tpfinal_gwt/servicioProducto</url-pattern>
  </servlet-mapping> 
     
  <servlet-mapping>
    <servlet-name>proveedorServlet</servlet-name>
    <url-pattern>/tpfinal_gwt/servicioProveedor</url-pattern>
  </servlet-mapping> 
  
  <servlet-mapping>
    <servlet-name>rolServlet</servlet-name>
    <url-pattern>/tpfinal_gwt/servicioRol</url-pattern>
  </servlet-mapping>  
  
  <servlet-mapping>
    <servlet-name>usuarioServlet</servlet-name>
    <url-pattern>/tpfinal_gwt/servicioUsuario</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>TPFinal_GWT.html</welcome-file>
  </welcome-file-list>

</web-app>
